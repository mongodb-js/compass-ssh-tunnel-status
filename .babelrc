{
  "env": {
    "production": {
      "presets": ["minify"]
    },
    "test": {
      "presets": [ ["env"] ],
      "plugins": [
        "react-hot-loader/babel",
        "syntax-object-rest-spread",
        "transform-class-properties"
      ]
    }
  },
  "presets": [ "react", ["env", { "modules": false }] ],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-object-rest-spread",
    "transform-class-properties"
  ]
}
